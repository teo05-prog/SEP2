<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="600.0" spacing="15.0"
      style="-fx-background-color: #cee0dc; -fx-padding: 40;"
      xmlns="http://javafx.com/javafx/23.0.1"
      fx:controller="view.admin.main.MainAdminViewController" xmlns:fx="http://javafx.com/fxml/1">

    <children>
        <ToolBar prefHeight="40.0" prefWidth="480.0" style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 10;">
            <items>
                <Button mnemonicParsing="false" style="-fx-background-color: #40798c; -fx-text-fill: white; -fx-background-radius: 6; -fx-padding: 6 12;" text="Trains" />
                <Button mnemonicParsing="false" style="-fx-background-color: #40798c; -fx-text-fill: white; -fx-background-radius: 6; -fx-padding: 6 12;" text="MyAccount" />
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="320.0" text="&lt;message&gt;" />
            </items>
        </ToolBar>

        <ListView prefHeight="340.0" prefWidth="480.0" style="-fx-background-color: white; -fx-background-radius: 10;" />

        <ButtonBar prefHeight="40.0" prefWidth="480.0" style="-fx-background-color: transparent; -fx-padding: 10;">
            <buttons>
                <Button mnemonicParsing="false" style="-fx-background-color: #40798c; -fx-text-fill: white; -fx-background-radius: 6; -fx-padding: 8 16;" text="Add" />
                <Button mnemonicParsing="false" style="-fx-background-color: #40798c; -fx-text-fill: white; -fx-background-radius: 6; -fx-padding: 8 16;" text="Remove" />
                <Button mnemonicParsing="false" style="-fx-background-color: #40798c; -fx-text-fill: white; -fx-background-radius: 6; -fx-padding: 8 16;" text="Modify" />
            </buttons>
        </ButtonBar>
    </children>

</VBox>
